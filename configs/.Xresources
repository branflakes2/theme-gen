##############
!! Xft settings
##############

Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

##colors##
*.background:	#0b1113
*.foreground:	#dee7ec
*.cursorColor:	#dee7ec
*.color1:	#0b1113
*.color2:	#141e23
*.color3:	#253640
*.color4:	#3a5665
*.color5:	#537b91
*.color6:	#7ea2b5
*.color7:	#b6cad5
*.color8:	#f2f5f7
*.color9:	#cdc3e0
*.color10:	#a1c2d4
*.color11:	#b9a9d6
*.color12:	#c7a694
*.color13:	#c38db0
*.color14:	#bbacd7
*.color15:	#e4d2c9
*.color16:	#92b5c8
##end colors##
################
!! URXVT settings
################

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0

!! transparency - true or false (default)
urxvt*transparent: false 

!! tint with any color; i.e., blue, red, tomato4, olivedrab2, etc.
!!   some nice listings are at:
!!     http://www.nisrv.com/drupal/?q=node/11
!!     http://www.htmlgoodies.com/tutorials/colors/article.php/3478921
URxvt*tintColor: #0a110c

!! shading - 0 to 99 darkens, 101 to 200 lightens.
!!   Don't use with tintColor; just use a darker or lighter color instead.
!! URxvt*shading: 15

URxvt*saveLines: 12000

!!URxvt*foreground: #a2a2a2
!!URxvt*background: #292929

!! Monaco Linux font
URxvt*font: xft:inconsolata:regular:pixelsize=22
URxvt*boldFont: xft:inconsolata:bold:pixelsize=22
URxvt*letterSpace: -1

!! scrollbar - true (default) or false
URxvt*scrollBar: false

!! scrollbar position - left=false (default) or right=true
URxvt*scrollBar_right: false

!! scrollbar style - rxvt (default), plain, next, or xterm
URxvt*scrollstyle: plain

urxvt*dynampicColors: on

!! perl extensions
URxvt*perl-ext-common: config-reload

